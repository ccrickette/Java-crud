<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="VehiclesOfInterestPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>vehiclesofinterestdesktop.ReasonForInterest</class>
    <class>vehiclesofinterestdesktop.VehicleModel</class>
    <class>vehiclesofinterestdesktop.VehicleOfInterest</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/VehiclesOfInterest"/>
      <property name="javax.persistence.jdbc.user" value="VehiclesOfInterest"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="voi"/>
    </properties>
  </persistence-unit>
</persistence>
